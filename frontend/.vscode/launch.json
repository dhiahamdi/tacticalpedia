    "version": "0.2.0",
    "configurations": [
    
        {
            "name": "launch cleaner",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "type": "chrome",
            "request": "attach",
            "name": "Attach to Chrome",
            "port": 4200,
            "webRoot": "${Desktop}"
        }
    ]
}